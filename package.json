{
  "name": "@any-hooks/solid",
  "version": "0.1.0",
  "description": "A high-quality & reliable Solid Hooks library.",
  "keywords": [
    "Solid",
    "Primitive",
    "solidjs",
    "Hooks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/any-hooks/solid-hooks"
  },
  "license": "MIT",
  "author": "pengzhanbo <volodymyr@foxmail.com> (https://github.com/pengzhanbo)",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      }
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs:dev": "vite",
    "docs:build": "vite build --debug",
    "build": "unbuild",
    "test": "vitest run",
    "test:unit": "vitest",
    "lint": "eslint .",
    "prepublishOnly": "vitest run && pnpm run build",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "bumpp package.json --execute=\"pnpm release:changelog\" --commit --all --push --tag && pnpm publish --access public"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "chrome > 40",
    "not ie 11"
  ],
  "prettier": "@pengzhanbo/prettier-config",
  "dependencies": {
    "intersection-observer": "^0.12.2",
    "js-cookie": "^3.0.5",
    "resize-observer-polyfill": "^1.5.1",
    "screenfull": "^6.0.2",
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config": "^0.4.3",
    "@pengzhanbo/prettier-config": "^0.4.3",
    "@pengzhanbo/utils": "^1.0.9",
    "@solidjs/router": "^0.8.3",
    "@solidjs/testing-library": "^0.8.4",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/js-cookie": "^3.0.4",
    "@types/markdown-it": "^13.0.2",
    "@types/node": "^20.8.6",
    "@types/throttle-debounce": "^5.0.0",
    "@unocss/reset": "^0.56.5",
    "autoprefixer": "^10.4.16",
    "bumpp": "^9.2.0",
    "classnames": "^2.3.2",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.51.0",
    "gray-matter": "^4.0.3",
    "jsdom": "^22.1.0",
    "markdown-it": "^13.0.2",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sass": "^1.69.4",
    "shiki": "^0.14.5",
    "solid-js": "^1.8.3",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.56.5",
    "vite": "^4.5.0",
    "vite-plugin-solid": "^2.7.2",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "solid-js": ">=1.7.0"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": "^16.0.0 || >=18"
  }
}
